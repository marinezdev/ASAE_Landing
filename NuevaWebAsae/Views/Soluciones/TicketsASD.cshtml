    <script>
        function ValidarDescarga()
        {
            var pathname = window.location.pathname;
            var arreglo = [];
            var obj = {};
            obj['Ingreso'] = pathname;

            arreglo.push(obj);

            var jsonObject = {
                "Usuario": arreglo
            };
            $.ajax({
                url: "@Url.Action("ComprobarSolicitudDW", "Registros")",
                async: false,
                type: "POST",
                data: JSON.stringify(jsonObject),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (response) {
                    $("#exampleModalCenter").modal("show");
                },
                success: function (response) {
                    if (response.Respuesta == 1) {
                        var lnk = document.getElementById('BtnIniciarDW');
                        if (lnk) {
                            lnk.click();
                        }
                    } else {
                        $("#exampleModalCenter").modal("show");
                    }
                }
            });
        }

        $(document).ready(function () {
            var timeSlide = 0;
            $('#timer').hide(0);
            $('#timer').css('display', 'none');
            $('#BtnRegistrarDW').click(function () {
                $('.box-success,.box-error').slideUp(timeSlide);
                setTimeout(function () {

                    if (validaFromDW()) {
                        if (ValidFormEmailDW()) {
                            var pathname = window.location.pathname;
                            var arreglo = [];
                            var obj = {};
                            obj['Nombre'] = $('#DW_Nombre').val();
                            obj['CorreoPersonal'] = $('#DW_EPer').val();
                            obj['TelefonoMovil'] = $('#DW_TMov').val();
                            obj['Ingreso'] = pathname;

                            arreglo.push(obj);

                            var jsonObject = {
                                "Usuario": arreglo
                            };

                            $.ajax({
                                url: "@Url.Action("SolicitudDW", "Registros")",
                                async: false,
                                type: "POST",
                                data: JSON.stringify(jsonObject),
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                error: function (response) {
                                    console.log(response);
                                    swal({
                                        title: "Algo ocurrió !",
                                        text: "Ha ocurrido un problema, inténtalo mas tarde ",
                                        icon: "warning",
                                        button: "Aceptar",
                                    });
                                },
                                success: function (response) {
                                    if (response.Respuesta == 1) {
                                        var lnk = document.getElementById('BtnIniciarDW');
                                        if (lnk) {
                                            lnk.click();
                                        }

                                        $("#BtnCerrarDW").trigger("click");

                                    } else {
                                        swal({
                                            title: "¡Ha ocurrido algo! ",
                                            text: response.RespuestaText,
                                            icon: "warning"
                                        });
                                    }
                                }
                            });
                        }
                    } else {
                        $.notify({
	                    icon: 'fa fa-paw',
	                    message: "Formulario incompleto."
                        },{
                            type: 'danger',
                            allow_dismiss: false,
                            animate: {
		                        enter: 'animated rollIn',
		                        exit: 'animated rollOut'
	                        }
                        });
                    }
                }, timeSlide);
                return false;
            });
        });

        function validaFromDW() {
            var respuesta = false;

            $('#DW_Nombre').css("border", "2px solid #FF5722");
            $('#DW_EPer').css("border", "2px solid #FF5722");
            $('#DW_TMov').css("border", "2px solid #FF5722");

            if ($('#DW_Nombre').val().length > 0) {
                $('#DW_Nombre').css("border", "2px solid #fff");
            }

            if ($('#DW_EPer').val().length > 0) {
                $('#DW_EPer').css("border", "2px solid #fff");
            }

            if ($('#DW_TMov').val().length > 0) {
                $('#DW_TMov').css("border", "2px solid #fff");
            }

            if ($('#DW_Nombre').val().length > 0) {
                if ($('#DW_EPer').val().length > 0) {
                    if ($('#DW_TMov').val().length > 0) {
                        respuesta = true;
                    }
                }
            }
            return respuesta;
        }

        function ValidFormEmailDW() {
            var respuesta = false;

            if (CorreoFromDW() == "ok") {
                $('#DW_EPer').css("border", "2px solid #fff");
                respuesta = true;
            } else {

                $('#DW_EPer').css("border", "2px solid #FF5722");

                $.notify({
                    icon: 'fa fa-paw',
                    message: "Correo electrónico invalido."
                }, {
                    type: 'danger',
                    allow_dismiss: false,
                    animate: {
                        enter: 'animated rollIn',
                        exit: 'animated rollOut'
                    }
                });
            }
            return respuesta;
        }

    </script>


    <script>

        $(document).on('click', ".shared", function () {
            var url = window.location.href;
            var title = document.title;

            url = encodeURIComponent(url);
            title = encodeURIComponent(title);

            //Boton Facebook
            if ($(this).hasClass('fb')) {
                url = 'https://www.facebook.com/sharer/sharer.php?u=' + url + '&t=' + title;

                //Boton Twitter
            } else if ($(this).hasClass('tt')) {
                user = 'asaeconsultores';//Nuestro usuario de twitter para mostrar como user
                url = 'https://twitter.com/intent/tweet?text=' + title + '&url=' + url + '&via=' + user;

                //Boyton Google Plus
            } else if ($(this).hasClass('gp')) {
                url = 'https://plus.google.com/share?url=' + url;

                //Boton WhtsApp
            } else if ($(this).hasClass('wa')) {
                url = 'https://api.whatsapp.com/send?text="' + document.title + '" - "' + window.location.href + '"';

                //Boton correo electronico
            } else if ($(this).hasClass('ce')) {
                url = 'mailto:?subject=' + title + '&body=' + url;

                //Ningun boton valido
            } else return;

            //Abrimos la url

            window.open(url.replace('+', '%2b'), '_blank');
        });


