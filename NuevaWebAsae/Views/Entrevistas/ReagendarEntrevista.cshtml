                                                <div class="col-md-12" id="">
                                                    <div class="demo-inline-spacing mt-3">
                                                        <div class="list-group">

                                                            @Html.Raw(ViewBag.Vacante.Contenido);

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                @*<p style="font-size:75%">Fecha de publicación: @ViewBag.vacante.FechaRegistro</p>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>

