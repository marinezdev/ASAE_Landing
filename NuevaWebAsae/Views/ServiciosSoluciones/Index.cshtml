            if ($('#Com_Email').val().length > 0) {
                $('#Com_Email').css("border", "2px solid #fff");
            }

            if ($('#Com_Telefono').val().length > 0) {
                $('#Com_Telefono').css("border", "2px solid #fff");
            }

            if ($('#Com_Empresa').val().length > 0) {
                $('#Com_Empresa').css("border", "2px solid #fff");
            }

